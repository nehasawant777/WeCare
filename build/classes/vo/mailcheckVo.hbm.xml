<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC 
"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
	<class name="vo.mailcheckVO" table="mailcheck">

		<id name="app_id" column="app_id" >
			<generator class="increment" /> 
		</id> 
		
		<property name="app_date">
		<column name="app_date"/>
		</property>	
		
		<property name="mails_sent">
		<column name="mails_sent"/>
		</property>		
		
<!-- <many-to-one name="abc" class="vo.insertVO" column="EIN" insert="false" update="false" ></many-to-one>	 -->	
		
</class>
		

</hibernate-mapping>